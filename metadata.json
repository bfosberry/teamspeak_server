{
  "info" : {
    "name" : "teamspeak3",
    "version" : "3.0.11.1",
    "arch" : "amd64",
    "docker_repo" : "bfosberry/teamspeak",
    "watcher_repo" : "bfosberry/teamspeak_watcher",
    "controller_repo" : "bfosberry/teamspeak_controller",
    "shares" : {
      "cpu_share" : 10,
      "mem_share" : 100,
      "cpu_shares_per_slot" : 10,
      "mem_shares_per_slot" : 20
    }
  },
  "provides" : [
    { "name" : "Username",
      "data_type" : "string",
      "key" : "username",
      "default_value" : "",
      "visible" : true
    },
    { "name" : "Password",
      "data_type" : "string",
      "key" : "password",
      "default_value" : "",
      "visible" : true
    },
    { "name" : "Admin Token",
      "data_type" : "string",
      "key" : "admin_token",
      "default_value" : "",
      "visible" : true
    },
    { "name" : "Error",
      "data_type" : "string",
      "key" : "error",
      "default_value" : "",
      "visible" : false
    }
  ],
  "responds_to" : [
    { "name": "Decom",
      "key": "decom",
      "args": []
    }
  ],
  "ports" : [
    { "port" : "9987",
      "protocol": "udp",
      "name": "Ts3 voice Port",
      "dynamic": true },
    { "port": "10011",
      "protocol": "tcp",
      "name" :"Ts3 query port",
      "dynamic": true },
    { "port": "30033",
      "protocol": "tcp",
      "name": "Ts3 file transfer port",
      "dynamic": true }
  ]
}
